{
	"extends": "@istanbuljs/nyc-config-typescript",
	"all": true,
	"check-coverage": true,
	"include": [
		"src"
	],
	"extension": [
		".ts",
		".tsx"
	],
	"exclude": [
		"**/*.d.ts"
	],
	"reporter": [
		"text",
		"lcov",
		"text-summary"
	],
		"branches": 100,
		"lines": 100,
		"functions": 100,
		"statements": 100
}